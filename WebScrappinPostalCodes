    "t_headers=[]\n",
    "for th in t.tbody.find_all(\"th\"):\n",
    "    # remove any newlines and extra spaces from left and right\n",
    "    headings.append(th.text.replace('\\n', ' ').strip())\n",
    "\n",
    "print(t_headers)"
   ]
